
.macro TEST_ALL filename
.data

L_test_all_name: .asciz "\n\filename\n"

.text
.p2align 2
	LOAD_ADDRESS x0, L_test_all_name
	bl print
.endm

.macro TEST_START testname
.align 2
\testname:
	str lr, [sp, #-16]!

	adr x0, L_TS_\testname
	bl print
  
	b L_TS_EXIT_\@

L_TS_\testname:
.asciz "* \testname - "

.align 2

L_TS_EXIT_\@:
	nop
.endm


.macro TEST_END
	ldr lr, [sp], #16
	ret
.endm
